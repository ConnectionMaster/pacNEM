
<div id="pacnem_lounge" class="row col-md-12">
    <div class="row">
        <div class="col-md-4">
            <h4 class="wrapper-title text-left">
                <i class="glyphicon glyphicon-user"></i>&nbsp;{{t "lounge.user_count_data_title"}}
            </h4>

            <table class="table table-hover">
                <thead style="display: none;"><th>&nbsp;</th><th>&nbsp;</th></thead>
                <tbody>
                    <tr>
                        <td class="text-center" style="border: 0;" colspan="2">
                            <span class="label label-info">
                                <i class="glyphicon glyphicon-globe"></i>&nbsp;
                                <span>{{lounge.sessions.total}} {{t "lounge.label_players_online"}}</span>
                            </span>
                        </td>
                    </tr>
                    <tr>
                        <td class="text-center" style="border: 0;" colspan="2">
                            <span class="label label-warning">
                                <i class="glyphicon glyphicon-glass"></i>&nbsp;
                                <span>{{lounge.sessions.lounge}} {{t "lounge.label_players_lounge"}}</span>
                            </span>
                        </td>
                    </tr>
                    <tr>
                        <td class="text-center" style="border: 0;" colspan="2">
                            <span class="label label-success">
                                <i class="glyphicon glyphicon-dashboard"></i>&nbsp;
                                <span>{{lounge.sessions.playing}} {{t "lounge.label_players_playing"}}</span>
                            </span>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>

        <div class="col-md-8">
            <h4 class="wrapper-title text-left">
                <i class="glyphicon glyphicon-star"></i>&nbsp;{{t "lounge.mosaics_data_title"}}
            </h4>

            <div class="container-fluid pacnem-lounge-mosaics">
                <ul style="margin: 0; padding: 0;">
                    {{#each lounge.mosaics.credits}}
                    <li style="display: inline-block;">
                        <div class="{{label}} label-mosaic" title="<i class='{{icon}}'></i> {{t title}}" data-toggle="tooltip">
                            <b>1&nbsp;<i class="{{icon}}"></i></b>
                        </div>
                    </li>
                    {{/each}}

                    {{#each lounge.mosaics.rewards.purchases}}
